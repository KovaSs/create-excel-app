{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": ["error", "always"],
    "comma-dangle": "off",
    "valid-jsdoc": "off",
    "no-unused-vars": "warn"
  }
}